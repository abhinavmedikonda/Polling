@model Polling.Models.Poll

<h2>@Model.Name</h2>

<div class="container">
    <div class="row container">
        @foreach (var item in Model.Items)
        {
            <div class="col-sm-2 well">
                <div>@item.Name</div>
                <div>@item.Votes.Count()</div>
            </div>
        }
    </div>
</div>
